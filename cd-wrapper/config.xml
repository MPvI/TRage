<?xml version='1.0' encoding='utf-8'?>
<widget id="com.ragingart.trage" version="2.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>TRage2</name>
    <description>
        A tool to manipulate the "TR181 Device Model" for Speedport HGs.
    </description>
    <author email="van_iterson@mail.de" href="http://github.com/ragingart">
        Mattheus Patrick van Iterson
    </author>
    <content src="index.html" />
    <icon src="www/assets/img/trage.png" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <engine name="android" spec="^6.2.3" />
    <engine name="browser" spec="^4.1.0" />
    <plugin name="com.scott.serviceDiscovery" spec="../../cordova-plugin-discovery" />
    <plugin name="cordova-plugin-file" spec="^4.3.3" />
    <plugin name="cordova-plugin-file-opener2" spec="^2.0.19" />
    <plugin name="cordova-plugin-network-information" spec="^1.3.3" />
    <plugin name="cordova-plugin-whitelist" spec="^1.3.2" />
</widget>
